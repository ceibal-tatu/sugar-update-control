#: ../src/model.py:69
#, python-format
msgid "%.0f KB"
msgstr "%.0f कि.बा."

#. TRANSLATORS: download size of updates, e.g. "2.3 MB"
#: ../src/model.py:72
#, python-format
msgid "%.1f MB"
msgstr "%.1f मे.बा."

