#: ../src/model.py:398
#, python-format
msgid "At version %s"
msgstr "संस्करण %s पर है"

#: ../src/model.py:401
#, python-format
#, python-format,
msgid "From version %(old)s to %(new)s (Size: %(size)s)"
msgstr "संस्करण %(old)s से %(new)s (आकार: %(size)s)"

#: ../src/model.py:453
#, python-format
msgid "New version %(version)s (Size: %(size)s)"
msgstr "नया संस्करण %(version)s (आकार: %(size)s)"

#: ../src/model.py:470
#, python-format
msgid "Checking %s..."
msgstr "%s जाँचा जा रहा है..."

#: ../src/model.py:611
msgid "New group"
msgstr "नया समूह"

#: ../src/model.py:776
#, python-format
msgid "%(number)d updates available.  Size: %(size)s"
msgstr "%(number)d अद्यतन उपलब्ध है.  आकार: %(size)s"

#: ../src/model.py:783
msgid "Setting the list of updates is not permitted."
msgstr "अद्यतनों की सूची सेट करने की अनुमति नहीं है."

