#: ../src/__init__.py:22 ../src/view.py:58
msgid "Software update"
msgstr "Cập nhật phần mềm"

#: ../src/view.py:186
msgid "_Uncheck All"
msgstr "_Bỏ chọn tất cả"

#: ../src/view.py:192
msgid "_Check All"
msgstr "_Chọn tất cả"

