#, python-format
msgid "At version %s"
msgstr "ही आव्रुत्ती %s"

#: ../src/model.py:401
#, python-format
#, python-format,
msgid "From version %(old)s to %(new)s (Size: %(size)s)"
msgstr "या आव्रुत्ती %(old)s ते %(new)s (परीमाण :%(size)s)"

#: ../src/model.py:453
#, python-format
msgid "New version %(version)s (Size: %(size)s)"
msgstr "नविन आव्रुत्ती %(version)s (परीमाण: %(size)s)"

#: ../src/model.py:470
#, python-format
msgid "Checking %s..."
msgstr "तपासत आहे %s..."

#: ../src/model.py:611
msgid "New group"
msgstr "नवीन ग्रुप"

#: ../src/model.py:776
#, python-format
msgid "%(number)d updates available.  Size: %(size)s"
msgstr "%(number)d अपडेटस उपलब्ध आहे.  परीमान: %(size)s"

#: ../src/model.py:783
msgid "Setting the list of updates is not permitted."
msgstr "अपडेटची यादि बनविण्यास परवानगी नाही."

#. this func is needed to make the 'install_update' key show up for '-l'
#: ../src/model.py:788
