msgid "Activity Group"
msgstr "سرکرمی گروپ"

#: ../src/model.py:331
msgid "Local activities"
msgstr "لوکل سرگرمی"

#: ../src/model.py:398
#, python-format
msgid "At version %s"
msgstr "پر ورژن %s"

#: ../src/model.py:401
#, python-format
#, python-format,
msgid "From version %(old)s to %(new)s (Size: %(size)s)"
msgstr "ورژن سے %(old)s سے %(new)s (سائز: %(size)s))"

#: ../src/model.py:453
#, python-format
msgid "New version %(version)s (Size: %(size)s)"
msgstr "نيا ورژن %(version)s (سائز: %(size)s)"

#: ../src/model.py:470
#, python-format
msgid "Checking %s..."
msgstr "ديکھنا %s..."

#: ../src/model.py:611
msgid "New group"
msgstr "نیا گروپ"

#: ../src/model.py:776
#, python-format
msgid "%(number)d updates available.  Size: %(size)s"
msgstr "%(number)d اپ ڈیٹ موجود ہے۔  سائز: %(size)s"

#: ../src/model.py:783
msgid "Setting the list of updates is not permitted."
msgstr "اپ ڈيٹ کی لسٹ کی سیٹنگز کی اجازت نہيں ہے۔"

#. this func is needed to make the 'install_update' key show up for '-l'
#: ../src/model.py:788
msgid "Only the 'set' operation for this key is defined."
msgstr "اس کی کا صرف سيٹ آپريشن ديا ہوا ہے۔"

#: ../src/model.py:801
msgid "More than one match found for the given activity name or id."
