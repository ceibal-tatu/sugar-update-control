#, python-format
msgid "Installing %s..."
msgstr "Gushyiramo %s..."

#: ../src/view.py:631
msgid "Could not access the network"
msgstr "Ntibishobotse kugera ku muyoboro"

#: ../src/view.py:633
msgid "Could not access the network to check for updates."
msgstr "Ntibishobotse kugera ku muyoboro ngo ugenzure ivugurura."

#: ../src/view.py:638
msgid "Your software is up-to-date"
msgstr "Software yawe yashizwe ku gihe"

#: ../src/view.py:641
#, python-format
msgid "You can install %s update"
msgid_plural "You can install %s updates"
msgstr[0] "Ushobora kwinjiza %s kigezweho"
msgstr[1] "Ushobora kwinjiza %s bigezweho"

#. TRANSLATORS: download size is 0
#: ../src/model.py:63
msgid "None"
msgstr "Nta nakimwe"

#. TRANSLATORS: download size of very small updates
#: ../src/model.py:66
msgid "1 KB"
msgstr "KB 1"

#. TRANSLATORS: download size of small updates, e.g. "250 KB"
#: ../src/model.py:69
#, python-format
msgid "%.0f KB"
msgstr "%.0f KB"

#. TRANSLATORS: download size of updates, e.g. "2.3 MB"
#: ../src/model.py:72
#, python-format
msgid "%.1f MB"
msgstr "%.1f MB"

