msgid "Loading groups..."
msgstr "Gushyiraho amatsinda..."

#: ../src/model.py:282
#, python-format
msgid "Fetching %s..."
msgstr "Kuvoma %s..."

#. add group header.
#: ../src/model.py:300
msgid "Activity Group"
msgstr "Itsinda ry'igikorwa"

#: ../src/model.py:331
msgid "Local activities"
msgstr "Ibikorwa by'umwimerere"

#: ../src/model.py:398
#, python-format
msgid "At version %s"
msgstr "Kuri verisiyo %s"

#: ../src/model.py:401
#, python-format
msgid "From version %(old)s to %(new)s (Size: %(size)s)"
msgstr "Kuva kuri verisiyo %(old)s kugera %(new)s (Ingano: %(size)s)"

#: ../src/model.py:453
#, python-format
msgid "New version %(version)s (Size: %(size)s)"
msgstr "Verisiyo nshyashya %(version)s (Ingano: %(size)s))"

#: ../src/model.py:470
#, python-format
msgid "Checking %s..."
msgstr "Kugenzura %s..."

#: ../src/model.py:611
msgid "New group"
msgstr "Itsinda rishya"

#: ../src/model.py:776
#, python-format
msgid "%(number)d updates available.  Size: %(size)s"
msgstr "%(number)d amavugurura ahari.  Ingano: %(size)s"

#: ../src/model.py:783
msgid "Setting the list of updates is not permitted."
msgstr "Gushyiraho urutonde rw'amavugurura ntabwo byemewe."

